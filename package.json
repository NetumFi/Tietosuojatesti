{
  "name": "olx",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "build": "ng build",
    "build_sv": "ng build --aot -prod --output-path dist/sv --locale sv --i18nFormat xlf --i18nFile ./src/locale/messages.sv.xlf",
    "build_fi": "ng build --aot -prod --output-path dist/fi --locale fi --i18nFormat xlf --i18nFile ./src/locale/messages.fi.xlf",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build_sv && npm run build_fi",
    "start": "node server",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "i18n": "ng-xi18n --i18nFormat xlf"
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/platform-server": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.2",
    "@ngrx/store-devtools": "^3.2.4",
    "angular2-uuid": "^1.1.1",
    "bootstrap": "^4.0.0-alpha.6",
    "classlist.js": "^1.1.20150312",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "ngrx-store-freeze": "^0.1.9",
    "ngx-bootstrap": "^1.6.6",
    "reselect": "^3.0.1",
    "rxjs": "^5.1.0",
    "snyk": "^1.41.1",
    "typescript": "~2.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/language-service": "^4.0.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.60",
    "codelyzer": "~2.0.0",
    "coveralls": "^2.13.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "2.0.0",
    "tslint": "^4.5.0"
  },
  "engines": {
    "node": "7.8.0",
    "npm": "4.2.0"
  },
  "snyk": true
}
